{
  "name": "bra-b",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "pnpm biome lint --write ./app ./functions ./server",
    "format": "pnpm biome format --write ./app ./functions ./server",
    "check": "pnpm biome check --write ./app ./functions ./server",
    "start": "wrangler pages dev ./build/client ./functions ./server",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "pnpm run build && wrangler pages dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@hono/vite-dev-server": "^0.16.0",
    "@remix-run/cloudflare": "^2.11.1",
    "@remix-run/cloudflare-pages": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "hono": "^4.6.1",
    "hono-remix-adapter": "^0.1.2",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@remix-run/dev": "^2.11.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
